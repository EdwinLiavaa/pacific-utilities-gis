Include "mapbasic.def"
Include "menu.def"

Declare Sub About

Sub About

OnError Goto Err_Manage

	Note "Copyright © 2011 SOPAC" + CHR$(13) + CHR$(10)

About:
	Exit Sub
	' The Exit Sub prevents the program from
	' unintentionally executing the error handlers
Err_Manage:
	Note "Error: " + str$(err()) + ", " + error$()
	Resume About
End Sub